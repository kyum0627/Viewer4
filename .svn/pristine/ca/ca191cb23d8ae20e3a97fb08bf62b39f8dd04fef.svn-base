            ("PhilippineSilver", new MyColor(178,178,178, 255)),
            ("Silver", new MyColor(188,188,188, 255)),
            ("SilverSand", new MyColor(198,198,198, 255)),
            ("LightrGrey", new MyColor(208,208,208, 255)),
            ("Alto", new MyColor(218,218,218, 255)),
            ("Mercury", new MyColor(228,228,228, 255)),
            ("BrightGrey", new MyColor(238, 238, 238, 1))
        }
        .ToDictionary(x => x.Name, x => x.color);

        public static MyColor GetColor3ValuesFromID(uint id)
        {
            if (IDtoName.TryGetValue(id, out string? colorName))
            {
                if (NametoMap.TryGetValue(colorName, out MyColor color))
                {
                    return color;
                }
            }
            return NametoMap["White"];
        }

        // 색상 ID를 색상 이름으로 변환하는 메서드
        public static string GetColorName(uint colorId)
        {
            // 색상 ID가 딕셔너리에 있는 경우 색상 이름을 반환
            return IDtoName.TryGetValue(colorId, out string? colorName)
                ? colorName
                : "Unknown";
        }

        // 색상 이름을 RGB 색상으로 변환하는 메서드
        public static MyColor GetColorFromName(string colorName)
        {
            // 색상 이름이 딕셔너리에 있는 경우 해당 RGB 색상 반환
            NametoMap.TryGetValue(colorName, out MyColor color);
            return color;
        }

        // 색상 이름과 색상 ID를 매핑하는 메서드
        public static uint GetColorIdFromName(string colorName)
        {
            // 색상 이름에 해당하는 색상 ID를 찾음
            return IDtoName
                .Where(pair => pair.Value.Equals(colorName, StringComparison.OrdinalIgnoreCase))
                .Select(pair => pair.Key)
                .FirstOrDefault();
        }

        // 색상 ID와 색상 이름의 매핑을 모든 ID와 이름 쌍으로 가져오는 메서드
        public static Dictionary<uint, string> GetColorIdToNameMapping() => IDtoName;

        public static MyColor GetColorValueFromID(uint colorId)
        {
            // 색상 이름을 ID로부터 가져옴
            string colorName = GetColorName(colorId);

            // 색상 이름을 사용하여 RGB 값을 가져옴
            return GetColorFromName(colorName);
        }
    }
}