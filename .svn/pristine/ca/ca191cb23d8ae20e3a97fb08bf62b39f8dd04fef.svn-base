using OpenTK.Mathematics;
using System;

namespace IGX.Geometry.DataStructure
{
    public struct MyColor
    {
        public byte R;
        public byte G;
        public byte B;
        public byte A;

        public Color4 ToBuffer()
        {
            return new Color4(R, G, B, A);
        }
        public static Vector4 ToBuffer(System.Drawing.Color color)
        {
            return new Vector4((float)color.R / 255, (float)color.G / 255, (float)color.B / 255, (float)color.A / 255);
        }
        public MyColor(byte r, byte g, byte b, byte a)
        {
            R = r;
            G = g;
            B = b;
            A = a;
        }
        public MyColor(System.Drawing.Color color)
        {
            R = color.R;
            G = color.G;
            B = color.B;
            A = color.A;
        }
    }

